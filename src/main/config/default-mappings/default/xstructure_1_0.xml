<?xml version="1.0" encoding="UTF-8"?>
<xstructure xmlns="http://plugins.intellij.net/xstructure"
            xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
            xsi:schemaLocation="http://plugins.intellij.net/xstructure http://plugins.intellij.net/xstructure/ns/xstructure_1_0.xsd"
            implType="default" name="xStructure" version="1.0" priority="10">
  <supported-schemas>
    <schema uri="http://plugins.intellij.net/xstructure/ns/xstructure_1_0.xsd"/>
  </supported-schemas>
  <mappings>
    <mapping match="/xstructure" label="{@implType} - v{@version}"/>
    <mapping match="/xstructure/supported-schemas/schema" label="{@uri}"/>
    <mapping match="/xstructure/mappings/mapping" label="{@match}"/>
  </mappings>
</xstructure>