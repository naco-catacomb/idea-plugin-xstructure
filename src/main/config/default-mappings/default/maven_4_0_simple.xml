<?xml version="1.0" encoding="UTF-8"?>
<xstructure xmlns="http://plugins.intellij.net/xstructure"
            xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
            xsi:schemaLocation="http://plugins.intellij.net/xstructure http://plugins.intellij.net/xstructure/ns/xstructure_1_1.xsd"
            implType="default" name="Maven POM / simple" version="1.0" priority="10">
  <supported-schemas>
    <schema uri="http://maven.apache.org/maven-v4_0_0.xsd"/>
  </supported-schemas>
  <mappings defaultSkip="all">
    <mapping match="/project" label="{name} - v{version}" tip="{description}"/>
    <mapping match="/project/developers"/>
    <mapping match="/project/developers/developer" label="{name}" icon="/fileTypes/custom.png"/>
    <mapping match="/project/dependencies" icon="/nodes/moduleGroupClosed.png"/>
    <mapping match="/project/build"/>
    <mapping match="/project/build/plugins" icon="/nodes/moduleGroupClosed.png"/>
    <mapping match="/project/build/plugins/plugin" label="{groupId}:{artifactId}" icon="/nodes/plugin.png"/>
    <mapping match="/project/dependencies/dependency" label="{groupId}:{artifactId}" icon="/objectBrowser/showLibraryContents.png"/>
    <mapping match="/project/(repositories|pluginRepositories)" icon="/nodes/moduleGroupClosed.png"/>
    <mapping match="/project/(repositories|pluginRepositories)/(repository|pluginRepository)" label="{id}" icon="/nodes/pluginnotinstalled.png"/>
    <mapping match="/project/profiles" icon="/nodes/moduleGroupClosed.png"/>
    <mapping match="/project/profiles/profile" label="{id}" icon="/actions/properties.png"/>
  </mappings>
</xstructure>
