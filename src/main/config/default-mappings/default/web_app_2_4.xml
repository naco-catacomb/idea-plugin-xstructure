<?xml version="1.0" encoding="UTF-8"?>
<xstructure xmlns="http://plugins.intellij.net/xstructure"
            xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
            xsi:schemaLocation="http://plugins.intellij.net/xstructure http://plugins.intellij.net/xstructure/ns/xstructure_1_1.xsd"
            implType="default" name="webapp" version="1.0" priority="10">
  <supported-schemas>
    <schema uri="http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd"/>
  </supported-schemas>
  <mappings>
    <mapping match="/web-app" label="{display-name}" tip="{description}"/>
    <mapping match="/web-app/(display-name|description)" skip="all"/>
    <mapping match="/web-app/listener" label="{listener-class}" skip="children"/>
    <mapping match="/web-app/context-param" label="{param-name}={param-value}" skip="children"/>
    <mapping match="/web-app/filter" label="{filter-name}" skip="children"/>
    <mapping match="/web-app/error-page" label="{error-code}{exception-type}" skip="children"/>
    <mapping match="/web-app/(servlet|servlet-mapping)" label="{servlet-name}" skip="children"
             tip="{description}"/>
    <mapping match="/web-app/(filter|servlet)/init-param" label="{param-name}={param-value}"
             skip="children"/>
    <mapping match="/web-app/(filter|servlet)/init-param" label="{param-name}={param-value}"
             skip="children"/>
    <mapping match="/web-app/jsp-config/taglib" label="{taglib-uri}" skip="children"/>
  </mappings>
</xstructure>