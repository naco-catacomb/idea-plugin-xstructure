<?xml version="1.0" encoding="UTF-8"?>
<xstructure xmlns="http://plugins.intellij.net/xstructure"
            xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
            xsi:schemaLocation="http://plugins.intellij.net/xstructure http://plugins.intellij.net/xstructure/ns/xstructure_1_0.xsd"
            implType="default" name="Hibernate Mapping" version="1.0" priority="10">
  <supported-schemas>
    <schema uri="http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd"/>
  </supported-schemas>
  <mappings>
    <mapping match="/hibernate-mapping/class" label="{@name}"/>
    <mapping match="/hibernate-mapping/class/id" label="{@name}" skip="children"/>
    <mapping match="/hibernate-mapping/class/.*-to-one" label="{@name}"/>
    <mapping match="/hibernate-mapping/class/.*-to-one/column" label="{@name}"/>
    <mapping match="/hibernate-mapping/class/property" label="{@name} [{@type}]"/>
    <mapping match="/hibernate-mapping/.*/column" label="{@name}"/>
    <mapping match="/hibernate-mapping/class/set" label="{@name}"/>
    <mapping match="/hibernate-mapping/class/set/key" skip="this"/>
    <mapping match="/hibernate-mapping/class/set/one-to-many" label="{@class}"/>
  </mappings>
</xstructure>