<?xml version="1.0" encoding="UTF-8"?>
<xstructure xmlns="http://plugins.intellij.net/xstructure"
            xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
            xsi:schemaLocation="http://plugins.intellij.net/xstructure http://plugins.intellij.net/xstructure/ns/xstructure_1_0.xsd"
            implType="default" name="jboss-portal-instances" version="1.2" priority="10">
  <supported-schemas>
    <schema uri="http://www.jboss.org/portal/dtd/portlet-instances_2_6.dtd"/>
  </supported-schemas>
  <mappings>
    <mapping match="/deployments/deployment" label="{instance/instance-id}"/>
    <mapping match="/deployments/deployment/instance" skip="children"/>
    <mapping match="/deployments/deployment/instance/preferences/preference" label="{name}" skip="children"/>
  </mappings>
</xstructure>