<?xml version="1.0" encoding="UTF-8"?>
<xstructure xmlns="http://plugins.intellij.net/xstructure"
            xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
            xsi:schemaLocation="http://plugins.intellij.net/xstructure http://plugins.intellij.net/xstructure/ns/xstructure_1_0.xsd"
            implType="default" name="jboss-portal-objects" version="1.0" priority="10">
  <supported-schemas>
    <schema uri="http://www.jboss.org/portal/dtd/portal-object_2_6.dtd"/>
  </supported-schemas>
  <mappings>
    <mapping match="/deployments/deployment/portal" label="portal-name"/>
    <mapping match=".*/page" label="page-name"/>
    <mapping match=".*/page/window" label="window-name"/>
    <mapping match=".*/page/properties/property" label="name"/>
    <mapping match=".*/page/page-name" skip="this"/>
  </mappings>
</xstructure>